{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test-coverage": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "test-ci": "ng test --no-watch --code-coverage --browsers=ChromeHeadlessNoSandBox",
    "lint": "ng lint",
    "lint-ci": "ng lint --eslint-config .eslintrc-ci.json",
    "e2e": "ng e2e --webdriverUpdate=false",
    "e2e-no-sandbox": "ng e2e --protractor-config e2e/protractor.conf.no.sandbox.js --webdriverUpdate=false",
    "e2e-ci": "ng e2e --protractor-config e2e/protractor.conf.ci.js --webdriverUpdate=false",
    "e2e-podman": "podman run --shm-size 2g -v $PWD/..:/Project --rm -it --network=host registry.gitlab.com/unizzan/oama/e2e-test bash -c \"cd frontend && npm run e2e-ci\"",
    "e2e-podman-ui": "podman run --shm-size 2g -e DISPLAY=$DISPLAY -v /tmp/.X11-unix:/tmp/.X11-unix -v $PWD/..:/Project --rm -it --network=host registry.gitlab.com/unizzan/oama/e2e-test bash -c \"cd frontend && npm run e2e-no-sandbox\"",
    "compodoc": "npx compodoc -p tsconfig.json src -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@ng-web-apis/common": "^1.9.0",
    "@ng-web-apis/intersection-observer": "^2.0.1",
    "@taiga-ui/cdk": "^2.9.1",
    "@taiga-ui/core": "^2.9.1",
    "@taiga-ui/icons": "^2.9.1",
    "@taiga-ui/kit": "^2.9.1",
    "@types/crypto-js": "^4.0.1",
    "crypto-js": "^3.3.0",
    "ng-inline-svg": "^12.1.0",
    "ngx-ellipsis": "^3.1.1",
    "rxjs": "~6.6.0",
    "socket.io-client": "^4.0.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.7.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "http-server": "^0.12.3",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "socket.io-mock": "^1.3.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
